package springbook.learningtest.templete;

public class TestMath {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int i = 1 ;
		int j = 1 ;
		
		int sum = 1;
		
		i += sum;
		i = i + sum;
		
		j =+ sum;
		j + i = sum;
		
		System.out.println("i===>" + i);
		System.out.println("j===>" + j);
	}

}
