DROP TABLE USERS IF EXISTS;

CREATE TABLE USERS
(
  ID        VARCHAR(10) NOT NULL,
  NAME      VARCHAR(20) NOT NULL,
  PASSWORD  VARCHAR(10) NOT NULL,
  RECOMMEND VARCHAR(10),
  LEVEL1    VARCHAR(10),
  LOGIN     VARCHAR(20),
  EMAIL     VARCHAR(30)
);

INSERT INTO USERS VALUES ('0001', 'Sample', 'DB Table Sample', '0791235', '20120914', '120150', 'Y', 10);
 	